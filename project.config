[access]
	inheritFrom = All-Projects
[submit]
	action = merge always
[access "refs/*"]
	owner = group user/ertugrul88
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group user/ertugrul88
	label-Verified = -1..+1 group user/ertugrul88
	push = group user/ertugrul88
	read = group user/ertugrul88
	rebase = group user/ertugrul88
	removeReviewer = group user/ertugrul88
	submit = group user/ertugrul88
	exclusiveGroupPermissions = label-Code-Review label-Verified push read rebase removeReviewer submit
[access "refs/heads/*"]
	create = group user/ertugrul88
	push = block +force group user/ertugrul88
	pushMerge = group user/ertugrul88
	read = group user/ertugrul88
	exclusiveGroupPermissions = create pushMerge read
[access "refs/tags/*"]
	push = group user/ertugrul88
	exclusiveGroupPermissions = push
[project]
[plugin "zuul-results-summary"]
	enabled = false
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
